{
	// Place your snippets for latex here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Anführungszeichen ›…‹": {
		"prefix": "dänisch einfach",
		"body": [ "\\danishsinglequote{$0}" ],
		"description": "Anführungszeichen (dänisch) einfach"
	},
	"Anführungszeichen »…«": {
		"prefix": "dänisch doppelt",
		"body": [ "\\danishdoublequote{$0}" ],
		"description": "Anführungszeichen (dänisch) doppelt"
	},
	"Anführungszeichen ‹…›": {
		"prefix": "französisch einfach",
		"body": [ "\\frenchsinglequote{$0}" ],
		"description": "Anführungszeichen (französisch) einfach"
	},
	"Anführungszeichen «…»": {
		"prefix": "französisch doppelt",
		"body": [ "\\frenchdoublequote{$0}" ],
		"description": "Anführungszeichen (französisch) doppelt"
	},
	"Anführungszeichen ‚…‘": {
		"prefix": "deutsch einfach",
		"body": [ "\\germansinglequote{$0}" ],
		"description": "Anführungszeichen (deutsch) einfach"
	},
	"Anführungszeichen „…“": {
		"prefix": "deutsch doppelt",
		"body": [ "\\germandoublequote{$0}" ],
		"description": "Anführungszeichen (deutsch) doppelt"
	},
	"Auslassungszeichen […]": {
		"prefix": "Auslassungszeichen",
		"body": [ "[…]" ],
		"description": "Auslassungszeichen"
	},
	"Buchtitel": {
		"prefix": "Buchtitel",
		"body": [ "\\textsc{$0}" ],
		"description": "Buchtitel",
	},
	"Fußnote": {
		"prefix": "Fußnote",
		"body": [ "\\footnote{$0}" ],
		"description": "Fußnote",
	},
	"Hervorhebung": {
		"prefix": "Hervorhebung",
		"body": [ "\\kursiv{$0}" ],
		"description": "Hervorhebung",
	},
	"Hochgestellt": {
		"prefix": "Hochgestellt",
		"body": [ "\\textsuperscript{$0}" ],
		"description": "Hochgestellt",
	},
	"Kommentar": {
		"prefix": "Kommentar",
		"body": [ "\\kommentar{$0}" ],
		"description": "Kommentar",
	},
	"Seitenzahl f.": {
		"prefix": "Seitenzahl f.",
		"body": [ "\\psq" ],
		"description": "Seitenzahl f.",
	},
	"Seitenzahl ff.": {
		"prefix": "Seitenzahl ff.",
		"body": [ "\\psqq" ],
		"description": "Seitenzahl ff.",
	},
	"Terminus": {
		"prefix": "Terminus",
		"body": [ "\terminus{$0}" ],
		"description": "Terminus",
	},
	"Vergleich": {
		"prefix": "Vergleich",
		"body": [ "\\cite[Vgl.][]{$0}" ],
		"description": "Vergleich",
	},
	"Zitat (Fließtext)": {
		"prefix": "Zitat (Fließtext)",
		"body": [ "\\enquote{$0}" ],
		"description": "Zitat (Fließtext)",
	},
	"Zitat (eingerückt)": {
		"prefix": "Zitat (eingerückt)",
		"body": [ "\\begin{quote}\n$0\n\\end{quote}" ],
		"description": "Zitat (eingerückt)",
	}
}